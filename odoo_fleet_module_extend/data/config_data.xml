<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Parámetro para el Endpoint de la API -->
        <record id="param_fleet_api_endpoint" model="ir.config_parameter">
            <field name="key">custom_fleet.api_endpoint</field>
            <field name="value">http://localhost:3000/api/vehicles</field>
        </record>

        <!-- Parámetro para la API Key -->
        <record id="param_fleet_api_key" model="ir.config_parameter">
            <field name="key">custom_fleet.api_key</field>
            <field name="value">clave-secreta</field>
        </record>

    </data>
</odoo>